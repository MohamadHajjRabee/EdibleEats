{
  "expo": {
    "name": "EdibleEats",
    "slug": "EdibleEats",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.mohamadhr.EdibleEats"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to identify edible wild plants for you.",
          "cameraPermission": "The app accesses your camera to let you take pictures of plants and use it ."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "765d434a-253d-4e35-93a6-53f536fded0b"
      }
    }
  }
}
